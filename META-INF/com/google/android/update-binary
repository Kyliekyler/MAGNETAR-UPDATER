#!/sbin/sh
require_new_magisk() {
  echo "- PLEASE INSTALL MAGISK V23+!"
  exit 1
}

umask 022
OUTFD=$2
ZIPFILE=$3
mount /data 2> /dev/null
[ -f /data/adb/magisk/util_functions.sh ] || require_new_magisk
source /data/adb/magisk/util_functions.sh
[ "$MAGISK_VER_CODE" -lt "20400" ] && require_new_magisk
install_module
exit 0
